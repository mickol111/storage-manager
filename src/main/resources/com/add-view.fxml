<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="460.0" prefWidth="415.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.controller.AddController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <VBox prefHeight="423.0" prefWidth="395.0" spacing="12.0">
         <children>
            <GridPane prefHeight="317.0" prefWidth="383.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label fx:id="titleText" text="Add" />
                  <Label contentDisplay="RIGHT" text="ID" GridPane.rowIndex="1" />
                  <Label contentDisplay="RIGHT" text="Name" GridPane.rowIndex="2" />
                  <Label contentDisplay="RIGHT" text="Color" GridPane.rowIndex="3" />
                  <Label contentDisplay="RIGHT" text="Brand" GridPane.rowIndex="4" />
                  <Label contentDisplay="RIGHT" text="Material" GridPane.rowIndex="5" />
                  <Label contentDisplay="RIGHT" text="Sex" GridPane.rowIndex="6" />
                  <Label fx:id="typeOut" text="Label" GridPane.columnIndex="1" />
                  <Label fx:id="idOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label fx:id="nameOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label fx:id="colorOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label fx:id="brandOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label fx:id="materialOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <Label fx:id="sexOut" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label text="Sizes" GridPane.rowIndex="7" />
                  <ListView fx:id="sizesListView" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <Label text="Size" GridPane.rowIndex="8" />
                  <Label text="Count" GridPane.rowIndex="9" />
                  <TextField fx:id="sizeInput" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                  <TextField fx:id="cntInput" GridPane.columnIndex="1" GridPane.rowIndex="9" />
               </children>
            </GridPane>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="12.0">
               <children>
                  <Button fx:id="acceptButton" mnemonicParsing="false" onAction="#acceptButton" text="Accept" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets left="12.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
